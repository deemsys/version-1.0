    <scm>
    	<connection>scm:svn:https://source.sakaiproject.org/svn/profile2/branches/profile2-1.5.x</connection>
    	<developerConnection>scm:svn:https://source.sakaiproject.org/svn/profile2/branches/profile2-1.5.x</developerConnection>
    	<url>https://source.sakaiproject.org/svn/profile2/branches/profile2-1.5.x</url>
  	</scm>
    
    <build>
        <plugins>
            <plugin>
                <groupId>com.mycila.maven-license-plugin</groupId>
                <artifactId>maven-license-plugin</artifactId>
                <version>1.5.0</version>
                <configuration>
                    <basedir>${basedir}</basedir>
                    <header>https://source.sakaiproject.org/svn/sakai/trunk/ECLv2-header-template.txt</header>
                    <quiet>false</quiet>
                    <failIfMissing>true</failIfMissing>
                    <aggregate>false</aggregate>
                    <includes>
                        <include>**/org/sakaiproject/**/*.java</include>
                        <include>**/org/sakaiproject/**/*.html</include>
                        <include>**/test/**/*.java</include>
                        <include>src/webapp/css/profile2-help.css</include>
                        <include>src/webapp/css/profile2-profile-entity.css</include>
                        <include>src/webapp/css/profile2.css</include>
                        <include>src/webapp/javascript/profile2.js</include>
                    </includes>
                    <excludes>
                        <exclude>target/**</exclude>
                        <exclude>m2-target/**</exclude>
                        <exclude>**/*.doc</exclude>
                        <exclude>**/*.properties</exclude>
                        <exclude>**/*.txt</exclude>
                        <exclude>**/*.xml</exclude>
                        <exclude>**/*.sql</exclude>
                    </excludes>
                    <useDefaultExcludes>true</useDefaultExcludes>
                    <useDefaultMapping>true</useDefaultMapping>
                    <properties>
                        <year>${project.inceptionYear}-2010</year>
                        <holder>The Sakai Foundation</holder>
                    </properties>
                    <encoding>UTF-8</encoding>
                </configuration>
                <!-- Disable: plugin ${year} variable causes build failure if a single year is encountered.  -->
                <!--
                <executions>
                    <execution>
                        <id>check-headers</id>
                        <phase>verify</phase>
                        <goals>
                            <goal>check</goal>
                        </goals>
                    </execution>
                </executions>
                -->
            </plugin>
            
            <!-- enforce Java 6 compiler -->
            <plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<compilerVersion>${sakai.jdk.version}</compilerVersion>
					<source>${sakai.jdk.version}</source>
					<target>${sakai.jdk.version}</target>
				</configuration>
			</plugin>
            
        </plugins>
    </build>
    
    <repositories> 
		<repository> 
			<id>sonatype-nexus-snapshots</id> 
			<name>Sonatype Nexus Snapshots</name> 
			<url> https://oss.sonatype.org/content/repositories/snapshots </url> 
			<releases> 
				<enabled>false</enabled> 
			</releases> 
			<snapshots> 
				<enabled>true</enabled> 
			</snapshots> 
		</repository> 

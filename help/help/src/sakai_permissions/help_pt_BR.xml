      <property name="location"><value>/sakai_permissions/arax_pt_BR.html</value></property>
   </bean>
   
   <bean id="siteInfoChangeRoles" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arfb_pt_BR</value></property>
      <property name="name"><value>Alterar papéis no Site Info</value></property>
      <property name="location"><value>/sakai_permissions/arfb_pt_BR.html</value></property>
   </bean>
   
   <bean id="worksiteSetupChangeRoles" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>ardq_pt_BR</value></property>
      <property name="name"><value>Alterar papéis no Worksite Setup</value></property>
      <property name="location"><value>/sakai_permissions/ardq_pt_BR.html</value></property>
   </bean>
   
   <bean id="participantRoles" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arbu_pt_BR</value></property>
      <property name="name"><value>Papéis do Participante</value></property>
      <property name="location"><value>/sakai_permissions/arbu_pt_BR.html</value></property>
   </bean>
   
   <bean id="permissionsRolesTools" class="org.sakaiproject.component.app.help.model.ResourceBean">

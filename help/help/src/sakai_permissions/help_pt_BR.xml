      <property name="name"><value>Controlar Acesso no Site Info</value></property>
      <property name="location"><value>/sakai_permissions/arci_pt_BR.html</value></property>
   </bean>
   
   <bean id="createWorksites" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>aqzr_pt_BR</value></property>
      <property name="name"><value>Criar/Adicionar/Editar/Excluir Worksites</value></property>
      <property name="location"><value>/sakai_permissions/aqzr_pt_BR.html</value></property>
   </bean>
   
   <bean id="worksiteSetupControlAccess" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>aqzw_pt_BR</value></property>
      <property name="name"><value>Controlar Acesso no Worksite Setup</value></property>
      <property name="location"><value>/sakai_permissions/aqzw_pt_BR.html</value></property>
   </bean>
   
   <bean id="changePermissions" class="org.sakaiproject.component.app.help.model.ResourceBean">

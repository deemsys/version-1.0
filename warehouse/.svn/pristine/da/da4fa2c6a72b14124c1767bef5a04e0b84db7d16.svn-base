<?xml version="1.0"?>
<assembly>
    <id>tomcat-overlay</id>
    <formats>
        <format>zip</format>
    </formats>
    <includeBaseDirectory>false</includeBaseDirectory>
    <dependencySets>
        <dependencySet>
            <outputDirectory>shared/lib/</outputDirectory>
            <useTransitiveDependencies>false
            </useTransitiveDependencies>
            <includes>
                <include>org.sakaiproject.warehouse:sakai-warehouse-api:jar:*</include>
            </includes>
            <unpack>false</unpack>
        </dependencySet>
        <dependencySet>
            <outputDirectory>components/sakai-warehouse-component</outputDirectory>
            <useTransitiveDependencies>false</useTransitiveDependencies>
            <includes>
                <include>org.sakaiproject.warehouse:sakai-warehouse-component:war:*</include>
            </includes>
            <unpack>true</unpack>
        </dependencySet>
    </dependencySets>
    <fileSets>
        <fileSet>
            <outputDirectory></outputDirectory>
            <includes>
                <include>${project.basedir}/README*</include>
            </includes>
        </fileSet>
    </fileSets>
</assembly>